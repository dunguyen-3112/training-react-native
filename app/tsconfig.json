{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@*": ["*"],
      "@components/*": ["./components/*"],
      "@screens/*": ["./screens/*"],
      "@assets/*": ["./assets/*"],
      "@constants/*": ["./constants/*"],
      "@utils/*": ["./utils/*"],
      "@types/*": ["./types/*"],
      "@hooks/*": ["./hooks/*"],
      "@navigation/*": ["./navigation/*"],
      "@__mock__/*": ["./__mock__/*"],
      "@helpers/*": ["./helpers/*"]
    }
  }
}
